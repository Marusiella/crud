@model Dao


<h2>Rejestracja</h2>
<form method="post">
    <label for="email">Email</label>
    <input type="email" name="email">
    <hr>
    <label for="password">Hasło</label>
    <input type="password" name="password">

    <button type="submit">Register</button>
</form>
<a asp-action="Login">Zaloguj się</a>